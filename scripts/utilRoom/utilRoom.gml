
function roomGetName(rm) {
	switch (rm) {
		case rm_Majorin: return "Majorin";
		case rm_Minoran: return "Minoran";
		case rm_Bask_Major: return "Bask Major";
		case rm_Bask_Minor: return "Bask Minor";
		case rm_Axion: return "Axion";
		case rm_Korda: return "Korda";
		case rm_Paradise: return "Paradise";
	}
	return "room";
}
