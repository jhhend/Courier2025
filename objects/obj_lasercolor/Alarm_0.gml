switch (dispcolor) {
    case c_red:
        dispcolor = c_orange; break;
    case c_orange:
        dispcolor = c_yellow; break;
    case c_yellow:
        dispcolor = c_lime; break;
    case c_lime:
        dispcolor = c_green; break;
    case c_green:
        dispcolor = c_teal; break;
    case c_teal:
        dispcolor = c_blue; break;
    case c_blue:
        dispcolor = c_purple; break;
    case c_purple:
        dispcolor = c_fuchsia; break;
    case c_fuchsia:
        dispcolor = c_red; break;
}

alarm[0] = room_speed;

